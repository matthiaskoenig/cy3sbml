    <listOfRules>
      <rateRule metaid="metaid_0000029" variable="X_gut">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <apply>
              <minus/>
              <ci> k_a </ci>
            </apply>
            <ci> X_gut </ci>
          </apply>
        </math>
            </rateRule>
      <rateRule metaid="metaid_0000030" variable="C_p">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <apply>
              <plus/>
              <apply>
                <minus/>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> k_a </ci>
                    <ci> F </ci>
                    <ci> X_gut </ci>
                  </apply>
                  <ci> V_C </ci>
                </apply>
                <apply>
                  <times/>
                  <ci> k12 </ci>
                  <ci> C_p </ci>
                </apply>
              </apply>
              <apply>
                <times/>
                <ci> k21 </ci>
                <ci> C_per </ci>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> k10 </ci>
              <ci> C_p </ci>
            </apply>
          </apply>
        </math>
            </rateRule>
      <rateRule metaid="metaid_0000031" variable="C_per">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> k12 </ci>
              <ci> C_p </ci>
            </apply>
            <apply>
              <times/>
              <ci> k21 </ci>
              <ci> C_per </ci>
            </apply>
          </apply>
        </math>
            </rateRule>
      <rateRule metaid="metaid_0000032" variable="C_e">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> k_eo </ci>
            <apply>
              <minus/>
              <ci> C_p </ci>
              <ci> C_e </ci>
            </apply>
          </apply>
        </math>
            </rateRule>
      <rateRule metaid="metaid_0000033" variable="C_t">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> k_tol </ci>
            <apply>
              <minus/>
              <ci> C_p </ci>
              <ci> C_t </ci>
            </apply>
          </apply>
        </math>
            </rateRule>
      <assignmentRule metaid="metaid_0000034" variable="E">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <plus/>
            <ci> E_0 </ci>
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> S </ci>
                <ci> C_e </ci>
              </apply>
              <apply>
                <plus/>
                <cn type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <ci> C_t </ci>
                  <ci> T_50 </ci>
                </apply>
              </apply>
            </apply>
          </apply>
        </math>
            </assignmentRule>
    </listOfRules>
